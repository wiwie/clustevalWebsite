<% content_for :title, "Gold Standard Configuration '" + @goldstandardConfig.name + "'" %>
<table id="goldstandardConfig" class="display">
  <thead>
	<tr>
	<th>Information</th>
	<th>Value</th>
	</tr>
  </thead>
  <tbody>
  <tr>
    <td>Absolute Path</td><td><%= @goldstandardConfig.abs_path.split('/')[-1] %></td>
  </tr>
  <tr>
    <td>Goldstandard</td><td><%= link_to @goldstandard.abs_path.split('/')[-2] + "/" + @goldstandard.abs_path.split('/')[-1], @goldstandard %></td>
  </tr>
  <tr>
    <td>File content</td><td>
<pre>
<%= @contents %>
</pre></td>
  </tr>
  </tbody>
</table>